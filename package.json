{
  "name": "react-redux-guard",
  "version": "0.0.1",
  "description": "",
  "author": {
    "name": "Andrew Natoli"
  },
  "main": "dist/module.js",
  "types": "dist/module.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "tsc --declaration",
    "dist:watch": "tsc  --declaration -w",
    "prepublish": "yarn dist"
  },
  "dependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "2.13.0",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^22.1.0",
    "@types/node": "^8.9.4",
    "@types/react": "^16.0.35",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^4.2.4",
    "@types/redux": "^3.6.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "ts-node": "^5.0.1",
    "typescript": "^2.6.2"
  },
  "jest": {
    "coverageReporters": ["lcov", "text-summary"],
    "coverageDirectory": "coverage",
    "testEnvironment": "jsdom",
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "testMatch": ["**/src/**/*.test.(ts|tsx)"],
    "collectCoverageFrom": ["**/src/**/*.(ts|tsx)", "!**/src/**/*.test.(ts|tsx)"],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AndrewNatoli/react-redux-guard"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "license": "MIT"
}
